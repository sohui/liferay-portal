{
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.16.1",
		"clay-checkbox": "2.16.1",
		"clay-dropdown": "2.16.1",
		"clay-icon": "2.16.1",
		"clay-management-toolbar": "2.16.1",
		"clay-modal": "2.16.1",
		"clay-radio": "2.16.1",
		"metal-clipboard": "2.0.1",
		"metal-debounce": "^2.0.2",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-jsx": "2.16.8",
		"metal-position": "2.1.2",
		"metal-state": "2.16.8",
		"object-hash": "^1.3.0"
	},
	"jest": {
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"fixtures/(.*)": "<rootDir>/test/js/__fixtures__/$1.js",
			"mock/(.*)": "<rootDir>/test/js/__mock__/$1.js",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/js/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/__fixtures__/",
			"/__mock__/",
			"/classes/"
		],
		"setupFiles": [
			"<rootDir>/jest-setup.config.js"
		],
		"setupFilesAfterEnv": [
			"<rootDir>jest-setup-after-env.config.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testPathIgnorePatterns": [
			"Evaluator",
			"FormBuilder",
			"FormRenderer",
			"LayoutProvider",
			"PageRenderer",
			"RuleBuilder",
			"RuleEditor",
			"RuleList",
			"Sidebar"
		]
	},
	"name": "dynamic-data-mapping-form-builder",
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts check",
		"format": "liferay-npm-scripts fix",
		"test": "metalsoy -s \"test/**/*.soy\" -d \"test\" && liferay-npm-scripts test"
	},
	"version": "3.0.0"
}